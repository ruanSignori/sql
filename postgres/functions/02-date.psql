-- Retorna a data atual no formato TIMESTAMP
SELECT NOW();

-- A idade desde a data passada como parâmetro
AGE(data_nascimento)

-- Receber somente o ano
EXTRACT(YEAR FROM AGE(data_nascimento))

-- Converter para formato BR
SELECT TO_CHAR(NOW(), 'DD/MM/YYYY HH:SS');

